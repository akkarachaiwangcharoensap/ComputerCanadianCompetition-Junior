package hiddenpalindrome;

import java.util.*;
import java.io.*;

/**
 *
 * @author 1251515
 */
public class HiddenPalindrome {

    public static void main(String[] args) {
        // Palindrome : A word that is the same when read forwards and when it is read backwards
        // Ex: anna, mom, wow
        // "a" is also a Palindrome
        
        Scanner userInput = new Scanner(System.in);
        
        String inputValue = userInput.nextLine();
        
        // b a n a n a
        // a n a n a
        
        // a n -> n a
        // a n a -> a n a
        
        
        String reverseInput = new StringBuffer(inputValue).reverse().toString();
        
        //System.out.println(reverseInput);
      
        if (inputValue.equals(reverseInput)) {
            System.out.println("It is Palindrome!");
        }else{
            for(int i = 1; i <= inputValue.length();i++){
                if (i % 3 == 0) {
                    String substring = inputValue.substring(i, inputValue.length());
                    String reverseSubstring = new StringBuffer(substring).reverse().toString();

                    System.out.println(substring);
                }
            }
        }
        
       for(int i = 10; i > 0; i--){
           System.out.println(i + ": " + (100 % i == 0));
       }
        
    }
    
}
